# Doxyfile конфигурация для TicTacToe
PROJECT_NAME           = "TicTacToe"
PROJECT_NUMBER         = 1.0
PROJECT_BRIEF          = "A flexible TicTacToe implementation"

OUTPUT_DIRECTORY       = docs
CREATE_SUBDIRS         = NO

INPUT                  = include src
FILE_PATTERNS          = *.h *.hpp *.cpp *.c
RECURSIVE              = YES

EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = YES
EXTRACT_STATIC         = YES
EXTRACT_LOCAL_CLASSES  = YES

SOURCE_BROWSER         = YES
INLINE_SOURCES         = NO

GENERATE_LATEX         = NO
GENERATE_HTML          = YES
GENERATE_TREEVIEW      = YES

ALWAYS_DETAILED_SEC    = YES
JAVADOC_AUTOBRIEF      = YES
QT_AUTOBRIEF           = YES

ENABLE_PREPROCESSING   = YES
MACRO_EXPANSION        = YES
EXPAND_ONLY_PREDEF     = YES

# Предопределенные макросы для C++
PREDEFINED            = DOXYGEN_SHOULD_SKIP_THIS